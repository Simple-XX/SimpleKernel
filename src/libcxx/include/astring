// Based on https://github.com/Alinshans/MyTinySTL
// astring for Simple-XX/SimpleKernel.

#ifndef _ASTRING_
#define _ASTRING_

// 定义了 string, wstring, u16string, u32string 类型

#include "basic_string"

namespace std {
    using string    = std::basic_string<char>;
    using wstring   = std::basic_string<wchar_t>;
    using u16string = std::basic_string<char16_t>;
    using u32string = std::basic_string<char32_t>;
};

#endif /* _ASTRING_ */
